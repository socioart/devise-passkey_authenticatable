= form_for(@passkey) do |f|
  = render_errors @passkey

  .form-group.mb-3
    = f.label :name, class: "form-label"
    = f.text_field :name, class: "form-control"

  .text-end
    = link_to t("back"), send("#{resource_name}_passkeys_path"), class: "btn btn-outline-secondary"
    = f.submit class: "btn btn-primary"
